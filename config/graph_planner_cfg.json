{
  "mode": "wsd",
  "enable_step3": true,
  "subgraph_total_cap": 800,
  "plan_k": 1,
  "max_nodes_per_anchor": 50,
  "candidate_total_limit": 200,
  "dir_diversity_k": 3,
  "prefer_test_files": true,
  "max_tfile_fraction": 0.6,
  "memory_caps": {
    "nodes": 200,
    "edges": 1000,
    "frontier": 50,
    "planner_tokens": 2000,
    "cgm_tokens": 16000
  },
  "lint": {
    "enabled": true,
    "framework": "auto"
  },
  "telemetry": {
    "events_path": "logs/events.jsonl",
    "test_runs_path": "logs/test_runs.jsonl"
  },
  "collate": {
    "mode": "wsd",
    "budget_tokens": 40000,
    "max_chunks": 64,
    "interleave_tests": true,
    "window_pad": 2,
    "enable_light_reorder": false,
    "per_file_max_chunks": 8
  },
  "cgm": {
    "enabled": false,
    "endpoint": null,
    "api_key_env": "CGM_API_KEY",
    "model": "cgm-default",
    "temperature": 0.2,
    "top_p": 0.9,
    "max_tokens": 2048,
    "timeout_s": 60,
    "model_path": "models/CodeFuse-CGM",
    "tokenizer_path": null,
    "max_input_tokens": 8192,
    "device": null,
    "device_map": null
  },
  "planner_model": {
    "enabled": false,
    "endpoint": null,
    "api_key_env": "PLANNER_MODEL_API_KEY",
    "model": "qwen2.5-coder-7b-instruct",
    "temperature": 0.2,
    "max_tokens": 1024,
    "top_p": 0.95,
    "timeout_s": 60,
    "system_prompt": null,
    "metadata": {},
    "model_path": "models/Qwen3-14B",
    "tokenizer_path": null,
    "max_input_tokens": 4096,
    "device": null,
    "device_map": null
  },
  "memlog": {
    "enabled": true
  }
}
